<div class="grid">
  <% @all.each do |item| %>
    <% case item %>
    <% when Gram %>
      <%= render 'gram', image: item %>
    <% when Post %>
      <%= render 'post', post: item %>
    <% when Tweet %>
      <%= render 'tweet', tweet: item %>
    <% end %>
  <% end %>
</div>
<script>
setTimeout( function() {
  var $grid = $('.grid').masonry({
    itemSelector: '.grid-item'
  });
}, 100);
</script>
